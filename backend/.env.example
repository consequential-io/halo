# AI Provider
AI_PROVIDER=gemini
GEMINI_MODEL=gemini-2.0-flash
GEMINI_API_KEY=your-gemini-api-key

# BigQuery
GCP_PROJECT=otb-dev-platform
BQ_DATASET=master

# Data Source
DATA_SOURCE=fixture  # fixture or bq
DATA_LOOKBACK_DAYS=30

# LLM Reasoning
ENABLE_LLM_REASONING=true
LLM_TIMEOUT_SECONDS=30

# Meta OAuth
META_APP_ID=3719964444932293
META_APP_SECRET=your-meta-app-secret
META_REDIRECT_URI=https://your-backend-url/auth/meta/callback

# API Auth
API_TOKEN=your-secret-api-token

# GCS Logging
GCS_BUCKET=halo-logs
GCS_EXECUTION_PATH=executions

# Frontend
FRONTEND_URL=http://localhost:5173

# Environment
ENVIRONMENT=development
